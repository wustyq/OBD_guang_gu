/* pages/cnbd/cnbd.wxss */
.sy{
  position: fixed;
  z-index: 999;
  bottom: 150rpx;
  right: 20rpx;
  height: 100rpx;
  width: 100rpx;
  background-color: rgba(89, 188, 245,.5);
  border-radius: 50rpx;
  text-align: center;
  line-height: 100rpx;
  color: rgb(175, 67, 81);
}
.biaoti {
  display: block;
  text-align: center;
  margin-top: 20rpx;
}
.in {
  display: flex;
  margin-top: 40rpx;
  border-bottom: 1px solid #e0e0e0;
  padding-bottom: 5px;
}
.in text {
  padding-left: 20rpx;
  width: 121px;
}
.in input {
  flex: 1;
  display: inline-block;
  margin-right: 20rpx;
  /* border: 2rpx solid #e0e0e0; */
  padding-left: 20rpx;
  color: rgb(20, 134, 187);
}
.in label {
  margin-left: 20rpx;
  font-size: 34rpx;
  color: red;
}
.btn {
  margin-top: 120rpx;
}
.btn button {
  margin-top: 20rpx;
}
.in .red {
  color: red;
}
.in .white {
  color: rgb(20, 134, 187);
}