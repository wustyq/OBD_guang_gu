.sy{
  position: fixed;
  z-index: 999;
  bottom: 150rpx;
  right: 20rpx;
  height: 100rpx;
  width: 100rpx;
  background-color: rgba(89, 188, 245,.5);
  border-radius: 50rpx;
  text-align: center;
  line-height: 100rpx;
  color: rgb(175, 67, 81);
}
.biaoti {
  display: block;
  text-align: center;
  margin-top: 20rpx;
}
.btn {
  margin-top: 120rpx;
}
.btn button {
  margin-top: 20rpx;
}
.section {
  display: flex;
  margin-top: 40rpx;
  border-bottom: 1px solid #e0e0e0;
  padding-bottom: 5px;
}
.section__title {
  display: inline-block;
  width: 100px;
  height: 24px;
  padding-left: 20rpx;
}
.picker {
  flex: 1;
  padding-left: 20rpx;
  /* border: 1rpx solid #e0e0e0; */
  color: rgb(12, 117, 197);
}
.picker view{
