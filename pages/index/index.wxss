/**index.wxss**/
page {
  height: 100%;
}
.container {
  display: flex;
  height: 100%;
  flex-direction: column;
}
.content {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.content swiper{
  height: 200rpx;
}
.content swiper image {
  height: 200rpx;
  width: 100vw;
}
.content .gongneng {
  flex: 1;
  background-color: #ffffff;
  display: flex;
  border-top: 1rpx solid #e0e0e0;
  padding-top: 40rpx;
}
.content .gongneng  .gn1{
  width: 150px;
  padding-left: 20rpx;
}
.content .gongneng  .gn2{
  width: 150px;
  padding-left: 20rpx;
}
.content .gongneng  .gn3{
  flex: 1;
}
.content .gongneng image {
  width: 150px;
  height: 100px;
}

.foot {
  display: flex;
  height: 40px;
  background-color: RGB(40,43,51);
}
.foot .active {
  background-color: RGB(57,61,73);
  color: white;
  border-bottom: 2px solid RGB(0,150,136);
}
.zhuye {
  flex: 1;
  text-align: center;
  line-height: 40px;
  color: RGB(196,197,201);
}
.wode {
  flex: 1;
  text-align: center;
  line-height: 40px;
  color: RGB(196,197,201);
}
.foot .logo {
  display: flex;
  flex-direction: column;
  width: 40px;
  height: 40px;
}
.foot .logo image {
  flex: 1;
  width: 40px;
  height: 40px;
}
